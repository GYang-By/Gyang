page {  
  width: 100%;
  display: block;
  background-color: #f5f5f5;
} 
.container{
  padding: 0;
  width: 100%;
}
.bgBox{
  width: 91.66%;
  background-color: #fff;
  margin-top: 86rpx;
  border-radius: 20rpx;
  text-align: center;
  padding-bottom: 60rpx;
}
.img{
  display: block;
  width: 130rpx;
  height: 130rpx;
  margin: -66rpx auto;
  position: relative;
}
.icon{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top:90rpx;
  left:90rpx;
}
.one{
  display: block;
  margin: 84rpx auto;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
  text-align: center;
}
.two,
.three{
  text-align: center;
  font-size: 26rpx;
  color: #999;
}
.three{
  margin-left: 20rpx;
}
.four{
  display: block;
  font-size: 50rpx;
  color: #2ab650;
  margin-top: 36rpx;
  padding-bottom: 59rpx;
  border-bottom: 1rpx dashed #ddd;
  position: relative;
}
.leftName{
  margin-left: 40rpx;
  font-size: 26rpx;
  color: #999;
  /* height: 390rpx; */
  width: 30%;
  float: left;
}
.leftName text{
  display: block;
  margin-top: 40rpx;
  line-height: 26rpx;
}
.rightName{
  float: right;
  margin-right: 40rpx;
  font-size: 26rpx;
  color: #666;
  /* height: 390rpx; */
  width: 55%;
  text-align: right;
}
.rightName text{
  display: block;
  height: 26rpx;
  margin-top: 40rpx;
  line-height: 26rpx;
}
.four::after,
.four::before{
  content: "";
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  position: absolute;
  background-color: #f5f5f5;
  top:110rpx;
}
.four::after{
  right: -25rpx;
}
.four::before{
  left: -25rpx;
}

.vedw{
  text-align: center
}